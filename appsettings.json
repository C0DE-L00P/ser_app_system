{
  "Data": {
    "Default": {
      "ConnectionString": "Filename=./App_Data/Ser_App_Default_v1.sqlite",
      "ProviderName": "Microsoft.Data.Sqlite"
    },
    "Northwind": {
      "ConnectionString": "Filename=./App_Data/Ser_App_Northwind_v1.sqlite",
      "ProviderName": "Microsoft.Data.Sqlite"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "CssBundling": {
    "Enabled": false,
    "Minimize": true,
    "UseMinCss": true
  },
